pipeline {
    agent any
    
    stages {
        stage('Checkout') {
            steps {
                echo  'Descargando...'
            }
        }
        stage('Build') {
            steps {
                echo  'Construyendo...'
            }
        }
        stage('Deploy') {
            steps {
                echo  'Desplegando...'
            }
        }
    }
}